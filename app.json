{
  "expo": {
    "name": "hodots",
    "slug": "hodotsapp-native",
    "version": "0.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.leafstudiosdot.hodots",
      "buildNumber": "0.0.3"
    },
    "android": {
      "splash": {
        "ldpi": "./assets/splash/android/ldpi-splash.png",
        "mdpi": "./assets/splash/android/mdpi-splash.png",
        "hdpi": "./assets/splash/android/hdpi-splash.png",
        "xhdpi": "./assets/splash/android/xhdpi-splash.png",
        "xxhdpi": "./assets/splash/android/xxhdpi-splash.png",
        "xxxhdpi": "./assets/splash/android/xxxhdpi-splash.png",
        "resizeMode": "cover"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.leafstudiosdot.hodots",
      "versionCode": 2,
      "permissions": ["BILLING", "CAMERA", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE", "RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
